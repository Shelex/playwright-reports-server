{
  "name": "playwright-reports-server",
  "version": "5.7.4",
  "description": "Playwright Reports Server: Can be used to accept blobs, merge them, store and view Playwright reports",
  "scripts": {
    "build": "npm run build:backend && npm run build:frontend",
    "build:backend": "cd backend && npm run build",
    "build:frontend": "cd frontend && npm run build",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && npm run dev",
    "start": "make start",
    "clean": "./scripts/cleanup-dev.sh",
    "format:check": "prettier --ignore-path .gitignore --list-different '**/**/*.{ts,tsx,json,md}'",
    "format:fix": "npm run format:check -- --write",
    "lint": "eslint . --ext .ts,.tsx",
    "test:unit": "jest",
    "test:api": "npx playwright test --project=api"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.932.0",
    "@aws-sdk/s3-request-presigner": "^3.932.0",
    "@heroui/link": "2.2.12",
    "@heroui/navbar": "2.2.13",
    "@heroui/react": "2.7.4",
    "@heroui/switch": "2.2.13",
    "@heroui/system": "2.4.11",
    "@heroui/theme": "2.4.11",
    "@playwright/test": "latest",
    "@react-aria/selection": "^3.12.1",
    "@react-aria/ssr": "^3.9.7",
    "@react-aria/utils": "^3.15.0",
    "@react-aria/visually-hidden": "^3.8.19",
    "@tanstack/react-query": "^5.59.15",
    "@tanstack/react-query-devtools": "^5.59.15",
    "better-sqlite3": "^12.4.1",
    "busboy": "^1.6.0",
    "clsx": "^2.1.1",
    "croner": "^9.0.0",
    "envalid": "^8.0.0",
    "framer-motion": "^11.11.8",
    "get-folder-size": "5.0.0",
    "jsonwebtoken": "^9.0.2",
    "jszip": "^3.10.1",
    "mime": "^4.0.4",
    "recharts": "^2.13.0",
    "sharp": "^0.33.5",
    "sonner": "^1.5.0",
    "tailwind-merge": "^3.0.2",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.8",
    "@cyborgtests/reporter-playwright-reports-server": "^2.3.3",
    "@types/better-sqlite3": "^7.6.13",
    "@types/busboy": "^1.5.4",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": ">=22.10.2",
    "@types/react": "18.3.11",
    "@types/uuid": "^10.0.0",
    "autoprefixer": "10.4.20",
    "concurrently": "^9.2.1",
    "dotenv": "^16.4.5",
    "jest": "^29.7.0",
    "postcss": "8.4.47",
    "tailwind-variants": "0.2.1",
    "tailwindcss": "3.4.13",
    "ts-node": "^10.9.2",
    "typescript": "5.2.2"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}
