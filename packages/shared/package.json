{
  "name": "@playwright-reports/shared",
  "version": "1.0.0",
  "description": "Shared types and utilities for Playwright Reports Server",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "typecheck": "tsc --noEmit",
    "lint": "biome check src/",
    "lint:fix": "biome check src/ --write",
    "format": "biome format src/ --write",
    "format:check": "biome format src/",
    "clean": "rm -rf dist"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/types/index.js"
    }
  },
  "files": [
    "dist/**/*"
  ],
  "peerDependencies": {
    "react": "^18.3.1"
  },
  "devDependencies": {
    "react": "^18.3.1",
    "typescript": "5.2.2"
  }
}